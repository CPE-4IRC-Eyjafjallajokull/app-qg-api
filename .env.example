# Core service settings
APP_NAME=app-qg-api
APP_VERSION=dev
APP_ENVIRONMENT=local
APP_DEBUG=false
APP_LOG_LEVEL=INFO
APP_CORS_ORIGINS=*
APP_EVENTS_PING_INTERVAL_SECONDS=25

# Database connections
POSTGRES_DSN=postgresql+asyncpg://postgres:postgres@localhost:5432/app

# Messaging
RABBITMQ_DSN=amqp://guest:guest@localhost:5672/

# Auth / Keycloak
AUTH_DISABLED=false

KEYCLOAK_SERVER_URL=http://localhost:8080
KEYCLOAK_REALM=master
KEYCLOAK_CLIENT_ID=app-qg-api
KEYCLOAK_AUDIENCE=app-qg-api
KEYCLOAK_CACHE_TTL_SECONDS=300
KEYCLOAK_TIMEOUT_SECONDS=3.0

# Nominatim reverse geocoding
NOMINATIM_BASE_URL=https://nominatim.openstreetmap.org
NOMINATIM_TIMEOUT_SECONDS=4.0
NOMINATIM_USER_AGENT=
NOMINATIM_ACCEPT_LANGUAGE=fr
NOMINATIM_CACHE_TTL_SECONDS=600
NOMINATIM_THROTTLE_SECONDS=1.0
NOMINATIM_CACHE_ROUNDING_PRECISION=5
